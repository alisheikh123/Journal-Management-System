    <div class="container">
        <div class="soc_details_head" id="heading"></div>
        <div class="info_body" @*style="overflow: auto;"*@>
            <div id="ORIC">
                <h3 style="text-align:center;">About ORIC </h3>
                <hr />

                <div class="row">
                    <div class="col-md-4">
                       
                        <img src="~/wwwroot/Image/oric/ORICLOGOFINAL.png" alt="ORIC" style="height: 150px; margin-left: 100px;" />
                    </div>
                    <div class="col-md-8" style="margin-top:30px;" >
                        <h1>
                            <strong>
                                Office of Research,<br />
                                Innovation and
                                <br />
                                Commercialization
                            </strong>

                        </h1>

                    </div>
